#############################################################################

####### Compiler, tools and options

CXX	=	$(SYSCONF_CXX) $(QT_CXX_MT)
CXXFLAGS=	$(SYSCONF_CXXFLAGS_QTOPIA) $(SYSCONF_CXXFLAGS_QT) $(SYSCONF_CXXFLAGS) $(SYSCONF_CXXFLAGS_LIB) -DQTOPIA_APP_INTERFACE
CC	=	$(SYSCONF_CC) $(QT_C_MT)
CFLAGS	=	$(SYSCONF_CFLAGS_QTOPIA) $(SYSCONF_CFLAGS) $(SYSCONF_CFLAGS_LIB) -DQTOPIA_APP_INTERFACE
INCPATH =	-I$(MOC_DIR)
LFLAGS	=	$(SYSCONF_LFLAGS_QTOPIA) $(SYSCONF_RPATH_QTOPIA) $(SYSCONF_LFLAGS_QT) $(SYSCONF_RPATH_QT) $(SYSCONF_LFLAGS) $(QT_LFLAGS_MT)
LIBS	=	$(SUBLIBS) -lqpe $(SYSCONF_LIBS_QTOPIA) $(SYSCONF_LIBS_QT) $(SYSCONF_LIBS_QTAPP)
MOC	=	$(SYSCONF_MOC)
UIC	=	$(SYSCONF_UIC)

####### Target

DESTDIR = $(QPEDIR)/plugins/inputmethods/
VER_MAJ = 1
VER_MIN = 0
VER_PATCH = 0
TARGET	= qpickboard
TARGET1 = lib$(TARGET).so.$(VER_MAJ)

####### Files

HEADERS =	pickboard.h \
		pickboardcfg.h \
		pickboardimpl.h \
		pickboardpicks.h
SOURCES =	pickboard.cpp \
		pickboardcfg.cpp \
		pickboardimpl.cpp \
		pickboardpicks.cpp
OBJECTS =	$(OBJECTS_DIR)/pickboard.o \
		$(OBJECTS_DIR)/pickboardcfg.o \
		$(OBJECTS_DIR)/pickboardimpl.o \
		$(OBJECTS_DIR)/pickboardpicks.o
INTERFACES =    
UICDECLS =      
UICIMPLS =      
SRCMOC	=	$(MOC_DIR)/moc_pickboard.cpp \
		$(MOC_DIR)/moc_pickboardcfg.cpp \
		$(MOC_DIR)/moc_pickboardpicks.cpp
OBJMOC	=	$(OBJECTS_DIR)/moc_pickboard.o \
		$(OBJECTS_DIR)/moc_pickboardcfg.o \
		$(OBJECTS_DIR)/moc_pickboardpicks.o


####### Implicit rules

.SUFFIXES: .cpp .cxx .cc .C .c

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules


all: $(DESTDIR)$(SYSCONF_LINK_TARGET)

$(DESTDIR)$(SYSCONF_LINK_TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) $(SUBLIBS)
	$(SYSCONF_LINK_LIB)

moc: $(SRCMOC)

tmake:
	tmake pickboard.pro

clean:
	-rm -f $(OBJECTS) $(OBJMOC) $(SRCMOC) $(UICIMPLS) $(UICDECLS)
	-rm -f *~ core
	-rm -f allmoc.cpp

####### Extension Modules

listpromodules:
	@echo 

listallmodules:
	@echo 

listaddonpromodules:
	@echo 

listaddonentmodules:
	@echo 


REQUIRES=

####### Sub-libraries


###### Combined headers



####### Compile

$(OBJECTS_DIR)/pickboard.o: pickboard.cpp \
		pickboard.h \
		pickboardpicks.h \
		pickboardcfg.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)/pickboard.o pickboard.cpp

$(OBJECTS_DIR)/pickboardcfg.o: pickboardcfg.cpp \
		pickboardcfg.h \
		pickboardpicks.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)/pickboardcfg.o pickboardcfg.cpp

$(OBJECTS_DIR)/pickboardimpl.o: pickboardimpl.cpp \
		pickboard.h \
		pickboardimpl.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)/pickboardimpl.o pickboardimpl.cpp

$(OBJECTS_DIR)/pickboardpicks.o: pickboardpicks.cpp \
		pickboardpicks.h \
		pickboardcfg.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)/pickboardpicks.o pickboardpicks.cpp

$(OBJECTS_DIR)/moc_pickboard.o: $(MOC_DIR)/moc_pickboard.cpp \
		pickboard.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)/moc_pickboard.o $(MOC_DIR)/moc_pickboard.cpp

$(OBJECTS_DIR)/moc_pickboardcfg.o: $(MOC_DIR)/moc_pickboardcfg.cpp \
		pickboardcfg.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)/moc_pickboardcfg.o $(MOC_DIR)/moc_pickboardcfg.cpp

$(OBJECTS_DIR)/moc_pickboardpicks.o: $(MOC_DIR)/moc_pickboardpicks.cpp \
		pickboardpicks.h \
		pickboardcfg.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)/moc_pickboardpicks.o $(MOC_DIR)/moc_pickboardpicks.cpp

$(MOC_DIR)/moc_pickboard.cpp: pickboard.h
	$(MOC) pickboard.h -o $(MOC_DIR)/moc_pickboard.cpp

$(MOC_DIR)/moc_pickboardcfg.cpp: pickboardcfg.h
	$(MOC) pickboardcfg.h -o $(MOC_DIR)/moc_pickboardcfg.cpp

$(MOC_DIR)/moc_pickboardpicks.cpp: pickboardpicks.h
	$(MOC) pickboardpicks.h -o $(MOC_DIR)/moc_pickboardpicks.cpp


