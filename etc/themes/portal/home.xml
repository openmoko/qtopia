<page name="home" base="themes/portal" color="#FFFFFF">
    <rect name="mainmenu" rect="0,9.54%,0,0" interactive="yes"/>
    <image name="background" rect="0,0,0,0" src="texture1" tile="yes" color="Background">
	<plugin name="bgplugin"/>
    </image>
	<image name="status" rect="24.3%,10.4%,51.14%x14.1%" scale="yes" src="titlebg_status">
      <status name="calendar" rect="0,12.90%,32.22%x67.74%" scale="yes" imageon="calendar_alert" onclick="coloron=Highlight" interactive="yes"/>
	    <status name="lock" rect="32.22%,35.48%,15.56%x45.16%" scale="yes" imageon="lock_on" onclick="coloron=Highlight" imageoff="lock_on" alphaoff="64" interactive="yes" on="expr:@/UI/KeyLock"/>
        <status name="alarm" rect="52.22%,35.48%,15.56%x45.16%" scale="yes" imageon="bell_on" onclick="coloron=Highlight" imageoff="bell_on" alphaoff="64" interactive="yes" on="expr:@/UI/DailyAlarm"/>
        <status name="calldivert" rect="70%,25.81%,15.56%x45.16%" scale="yes" imageon="calldivert" onclick="coloron=Highlight" interactive="yes" on="expr:@/Telephony/Status/CallDivert"/>
	</image>
    <image name="calls" rect="0,11.36%,46.59%x42.27%" src="alert_call" scale="yes" transient="yes" active="expr:@/Communications/Calls/MissedCalls > 0" interactive="yes">
      <image src="calloverlay" color="Highlight" scale="yes"/>
	  <text name="calls" size="12" bold="yes" rect="54.88%,45.16%,28.05%x21.51%" short="yes" color="HighlightedText" interactive="yes">
	    expr:@/Communications/Calls/MissedCalls
	  </text>
	  <image rect="6.1%,19.35%,39.02%x60.22%" scale="yes" src="call" interactive="yes"/>
    </image>
    <image name="messages" rect="-46.59%,11.36%,46.59%x42.27%" src="alert_message" scale="yes" transient="yes" active="expr:@/Communications/Messages/NewMessages > 0" interactive="yes">
	<image src="messageoverlay" color="Highlight" scale="yes"/>
	<text name="messages" size="12" bold="yes" rect="12.19%,45.16%,34.15%x21.51%" short="yes" color="HighlightedText" align="right" interactive="yes">
	    expr:@/Communications/Messages/NewMessages
	</text>
    <image rect="52.44%,23.66%,36.59%x55.91%" scale="yes" src="message" interactive="yes"/>
    </image>
    <image name="dialer" rect="57,106,-58,-52"  keypad="no" src="default/dialerbutton" onclick="src=default/dialerbuttonp;color=Highlight" interactive="yes" scale="yes"/>
    <text name="operator" rect="0,-41,0x16" size="7" outline="#FFFFFF" color="#000000" align="hcenter">
	expr:@/Telephony/Status/OperatorName
    </text>
    <layout rect="0,-25,0x14" align="hcenter">
	<group name="profile" rect="0,0,98x14" interactive="yes" transient="yes" active="expr:!@/UI/Profile/Default">
	    <image name="profile" rect="0,0,98x14" src="profile" onclick="src=profile-pressed;color=Highlight"/>
	    <text name="profile" size="6" rect="4,0,90x12" align="hcenter" onclick="color=HighlightedText">
		expr:@/UI/Profile/Name
	    </text>
	</group>
    </layout>
    <rect name="infobox" rect="0,-75,0x50" color="#72715a" brush="Base" transient="yes" active="no">
	<image name="infobox" rect="0,0,40x40"/>
	<text name="infobox" rect="40,0,-3,50" size="7" color="Text" align="hcenter,vcenter"/>
    </rect>
    <layout name="pinbox" rect="0,30,0,0" orientation="vertical" transient="yes" active="no">
	<rect name="pinbox" rect="0,0,0,32" color="#72715a" brush="Base">
	    <image name="pinbox" rect="0,0,32x32"/>
	    <text name="pinbox" rect="32,0,-3,50" size="7" color="Text" align="hcenter,vcenter"/>
	</rect>
	<layout name="vertical" rect="22,0,132x132" orientation="vertical" spacing="-1" align="hcenter">
	    <layout name="horizontal" rect="0,0,0x33" orientation="horizontal" spacing="-1" align="hcenter">
		<group rect="0,0,44x33" name="one" interactive="yes">
		    <image rect="0,0,0,0" name="one_bg" src="default/r1" onclick="src=default/r1p" scale="yes" />
		    <image rect="5,6,-5,-5" name="one_fg" src="default/1" scale="yes"/>
		</group>
		<group rect="0,0,44x33" name="two" interactive="yes">
		    <image rect="0,0,0,0" name="two_bg" src="default/r1" onclick="src=default/r1p" scale="yes" />
		    <image rect="5,6,-5,-5" name="two_fg" src="default/2" scale="yes"/>
		</group>
		<group rect="0,0,44x33" name="three" interactive="yes">
		    <image rect="0,0,0,0" name="three_bg" src="default/r1" onclick="src=default/r1p" scale="yes" />
		    <image rect="5,6,-5,-5" name="three_fg" src="default/3" scale="yes"/>
		</group>
	    </layout>
	    <layout rect="0,0,0x33" orientation="horizontal" spacing="-1" align="hcenter">
		<group rect="0,0,44x33" name="four" interactive="yes">
		    <image rect="0,0,0,0" name="four_bg" src="default/r2" onclick="src=default/r2p" scale="yes" />
		    <image rect="5,6,-5,-5" name="four_fg" src="default/4" scale="yes"/>
		</group>
		<group rect="0,0,44x33" name="five" interactive="yes">
		    <image rect="0,0,0,0" name="five_bg" src="default/r2" onclick="src=default/r2p" scale="yes" />
		    <image rect="5,6,-5,-5" name="five_fg" src="default/5" scale="yes"/>
		</group>
		<group rect="0,0,44x33" name="six" interactive="yes">
		    <image rect="0,0,0,0" name="six_bg" src="default/r2" onclick="src=default/r2p" scale="yes" />
		    <image rect="5,6,-5,-5" name="six_fg" src="default/6" scale="yes"/>
		</group>
	    </layout>
	    <layout rect="0,0,0x33" orientation="horizontal" spacing="-1" align="hcenter">
		<group rect="0,0,44x33" name="seven" interactive="yes">
		    <image rect="0,0,0,0" name="seven_bg" src="default/r3" onclick="src=default/r3p" scale="yes" />
		    <image rect="5,6,-5,-5" name="seven_fg" src="default/7" scale="yes"/>
		</group>
		<group rect="0,0,44x33" name="eight" interactive="yes">
		    <image rect="0,0,0,0" name="eight_bg" src="default/r3" onclick="src=default/r3p" scale="yes" />
		    <image rect="5,6,-5,-5" name="eight_fg" src="default/8" scale="yes"/>
		</group>
		<group rect="0,0,44x33" name="nine" interactive="yes">
		    <image rect="0,0,0,0" name="nine_bg" src="default/r3" onclick="src=default/r3p" scale="yes" />
		    <image rect="5,6,-5,-5" name="nine_fg" src="default/9" scale="yes"/>
		</group>
	    </layout>
	    <layout rect="0,0,0x33" orientation="horizontal" spacing="-1" align="hcenter">
		<group rect="0,0,44x33" name="star" interactive="yes">
		    <image rect="0,0,0,0" name="star_bg" src="default/r4" onclick="src=default/r4p" scale="yes" />
		    <image rect="5,6,-5,-5" name="star_fg" src="default/star" scale="yes"/>
		</group>
		<group rect="0,0,44x33" name="zero" interactive="yes">
		    <image rect="0,0,0,0" name="zero_bg" src="default/r4" onclick="src=default/r4p" scale="yes" />
		    <image rect="5,6,-5,-5" name="zero_fg" src="default/0" scale="yes"/>
		</group>
		<group rect="0,0,44x33" name="hash" interactive="yes">
		    <image rect="0,0,0,0" name="hash_bg" src="default/r4" onclick="src=default/r4p" scale="yes" />
		    <image rect="5,6,-5,-5" name="hash_fg" src="default/hash" scale="yes"/>
		</group>
	    </layout>
	</layout>
    </layout>
    <layout name="call_active" rect="0,-125,0x61" align="hcenter" transient="yes" active="no" interactive="yes">
    </layout>
</page>
