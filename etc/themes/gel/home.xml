<page name="home" base="themes/gel" color="#FFFFFF">
    <image name="background" scale="yes" rect="0,21,-1,-14" src="bground" color="Background">
	<plugin name="bgplugin"/>
    </image>
    <layout rect="0,27,0x21" align="hcenter">
	<image name="status" rect="0,0,85x21" src="iconbar" color="Highlight">
	    <status name="lock" rect="12,1,14x16" imageon="lock" imageoff="lock" onclick="coloron=Highlight" alphaoff="64" interactive="yes" on="expr:@/UI/KeyLock"/>
	    <status name="alarm" rect="34,1,16x16" imageon="bell" imageoff="bell" onclick="coloron=Highlight" alphaoff="64" interactive="yes" on="expr:@/UI/DailyAlarm"/>
	    <status name="calldivert" rect="57,2,17x13" imageon="divert" imageoff="divert" alphaoff="64" onclick="coloron=Highlight" interactive="yes" on="expr:@/Telephony/Status/CallDivert"/>
	</image>
    </layout>
    <image name="calls" rect="0,27,86x83" src="newmsg" color="Highlight" alpha="192" transient="yes" active="expr:@/Communications/Calls/MissedCalls > 0" interactive="yes">
	<text name="calls" size="7" rect="8,44,70x36" color="HighlightedText" format="RichText" align="hcenter" interactive="yes">
	    <tr><trtext>%1 missed call(s)</trtext><trarg>@/Communications/Calls/MissedCalls</trarg></tr>
	</text>
	<image rect="4,10,20x34" src="phone" interactive="yes"/>
    </image>
    <image name="messages" rect="-88,26,88x84" src="missed" color="Highlight" alpha="192" transient="yes" active="expr:@/Communications/Messages/NewMessages > 0" interactive="yes">
	<text name="messages" size="7" rect="8,44,72x36" color="HighlightedText" format="RichText" align="hcenter" interactive="yes">
	    <tr><trtext>%1 new message(s)</trtext><trarg>@/Communications/Messages/NewMessages</trarg></tr>
	</text>
	<image rect="54,16,22x22" src="email" interactive="yes"/>
    </image>
    <image name="dialer" rect="57,106,-58,-52"  keypad="no" src="default/dialerbutton" onclick="src=default/dialerbuttonp;color=Highlight" interactive="yes" scale="yes"/>
    <text name="operator" rect="0,-55,0x16" size="7" outline="#FFFFFF" color="#000000" align="hcenter">
	expr:@/Telephony/Status/OperatorName
    </text>
    <layout rect="0,-35,0x18" align="hcenter">
	<group name="profile" rect="0,0,98x18" interactive="yes" transient="yes" active="expr:!@/UI/Profile/Default">
	    <image name="profile" rect="0,0,99x18" src="profile" color="Highlight" onclick="src=profile-pressed;color=Highlight"/>
	    <text name="profile" size="6" rect="4,1,90x16" align="hcenter,vcenter" color="HighlightedText" onclick="color=Text" shadow="200">
		expr:@/UI/Profile/Name
	    </text>
	</group>
    </layout>
    <group name="infobox" rect="0,-96,0x61" transient="yes" active="no">
        <image src="box" color="Highlight" alpha="192"/>
	<image name="infobox" rect="6,0,40x61"/>
	<text name="infobox" rect="48,0,-5,61" size="7" color="HighlightedText" align="hcenter,vcenter"/>
    </group>
    <layout name="pinbox" rect="0,27,0,0" orientation="vertical" transient="yes" active="no">
	<rect name="pinbox" rect="0,0,0,32" color="#72715a" brush="Base">
	    <image name="pinbox" rect="0,0,32x32"/>
	    <text name="pinbox" rect="32,0,-3,50" size="7" color="Text" align="hcenter,vcenter"/>
	</rect>
	<layout name="vertical" rect="22,0,132x132" orientation="vertical" spacing="-1" align="hcenter">
	    <layout name="horizontal" rect="0,0,0x33" orientation="horizontal" spacing="-1" align="hcenter">
		<group rect="0,0,44x33" name="one" interactive="yes">
		    <image rect="0,0,0,0" name="one_bg" src="button" onclick="src=buttonp" scale="yes" />
		    <image rect="5,6,-5,-5" name="one_fg" src="default/1" scale="yes"/>
		</group>
		<group rect="0,0,44x33" name="two" interactive="yes">
		    <image rect="0,0,0,0" name="two_bg" src="button" onclick="src=buttonp" scale="yes" />
		    <image rect="5,6,-5,-5" name="two_fg" src="default/2" scale="yes"/>
		</group>
		<group rect="0,0,44x33" name="three" interactive="yes">
		    <image rect="0,0,0,0" name="three_bg" src="button" onclick="src=buttonp" scale="yes" />
		    <image rect="5,6,-5,-5" name="three_fg" src="default/3" scale="yes"/>
		</group>
	    </layout>
	    <layout rect="0,0,0x33" orientation="horizontal" spacing="-1" align="hcenter">
		<group rect="0,0,44x33" name="four" interactive="yes">
		    <image rect="0,0,0,0" name="four_bg" src="button" onclick="src=buttonp" scale="yes" />
		    <image rect="5,6,-5,-5" name="four_fg" src="default/4" scale="yes"/>
		</group>
		<group rect="0,0,44x33" name="five" interactive="yes">
		    <image rect="0,0,0,0" name="five_bg" src="button" onclick="src=buttonp" scale="yes" />
		    <image rect="5,6,-5,-5" name="five_fg" src="default/5" scale="yes"/>
		</group>
		<group rect="0,0,44x33" name="six" interactive="yes">
		    <image rect="0,0,0,0" name="six_bg" src="button" onclick="src=buttonp" scale="yes" />
		    <image rect="5,6,-5,-5" name="six_fg" src="default/6" scale="yes"/>
		</group>
	    </layout>
	    <layout rect="0,0,0x33" orientation="horizontal" spacing="-1" align="hcenter">
		<group rect="0,0,44x33" name="seven" interactive="yes">
		    <image rect="0,0,0,0" name="seven_bg" src="button" onclick="src=buttonp" scale="yes" />
		    <image rect="5,6,-5,-5" name="seven_fg" src="default/7" scale="yes"/>
		</group>
		<group rect="0,0,44x33" name="eight" interactive="yes">
		    <image rect="0,0,0,0" name="eight_bg" src="button" onclick="src=buttonp" scale="yes" />
		    <image rect="5,6,-5,-5" name="eight_fg" src="default/8" scale="yes"/>
		</group>
		<group rect="0,0,44x33" name="nine" interactive="yes">
		    <image rect="0,0,0,0" name="nine_bg" src="button" onclick="src=buttonp" scale="yes" />
		    <image rect="5,6,-5,-5" name="nine_fg" src="default/9" scale="yes"/>
		</group>
	    </layout>
	    <layout rect="0,0,0x33" orientation="horizontal" spacing="-1" align="hcenter">
		<group rect="0,0,44x33" name="star" interactive="yes">
		    <image rect="0,0,0,0" name="star_bg" src="button" onclick="src=buttonp" scale="yes" />
		    <image rect="5,6,-5,-5" name="star_fg" src="default/star" scale="yes"/>
		</group>
		<group rect="0,0,44x33" name="zero" interactive="yes">
		    <image rect="0,0,0,0" name="zero_bg" src="button" onclick="src=buttonp" scale="yes" />
		    <image rect="5,6,-5,-5" name="zero_fg" src="default/0" scale="yes"/>
		</group>
		<group rect="0,0,44x33" name="hash" interactive="yes">
		    <image rect="0,0,0,0" name="hash_bg" src="button" onclick="src=buttonp" scale="yes" />
		    <image rect="5,6,-5,-5" name="hash_fg" src="default/hash" scale="yes"/>
		</group>
	    </layout>
	</layout>
    </layout>


</page>
