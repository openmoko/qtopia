		    </group>
		    <group rect="0,0,105x80" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),9">
			    <image scale="yes" src="dialer_btn" color="#006492" onclick="src=dialer_btn_pressed" interactive="yes" />
			    <text rect="0,12,100%x50%" align="hcenter,bottom" bold="yes" color="#FFFFFF" size="20">9</text>
