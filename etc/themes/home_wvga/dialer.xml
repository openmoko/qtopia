<?xml version="1.0"?>
<page xmlns="http://www.trolltech.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.trolltech.com themedview.xsd" name="dialer" transparent="yes" base="themes/deskphone" keypad="no">

  <image rect="50,16,630x75" src="number_bg" scale="yes"/>
  <dialer-lineedit-plugin name="dialernumber" rect="60,21,605x67" colorGroup="Background=#FFFFFF;Base=#DDDDDD;Text=#000000" bold="yes" size="20"/>
    <layout rect="430,105,250x270" orientation="vertical" spacing="10">
      <layout rect="0,0,0x60" orientation="horizontal" spacing="10" align="hcenter">
        <group rect="0,0,120x60" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,clear()">
		<image src="dialer_btn" color="#7e0000" onclick="src=dialer_btn_pressed" interactive="yes" scale="yes" />
          <text rect="0,0,100%x100%" bold="yes" color="#FFFFFF" size="8" align="hcenter,vcenter">Clear</text>
        </group>
        <group rect="0,0,120x60" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,backspace()">
          <image src="dialer_btn" color="#7e0000" onclick="src=dialer_btn_pressed" interactive="yes" scale="yes" />
          <image rect="25,13,70x44" src="backspace.svg"/>
        </group>
      </layout>
      <group rect="0,0,250x60" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,saveToContact()">
	      <image scale="yes" src="big_dialer_btn" color="#006492" onclick="src=big_dialer_btn_pressed" interactive="yes" />
        <text rect="0,0,50%x100%" align="right,vcenter" bold="yes" color="#FFFFFF" size="8">Add</text>
        <image src="launch_contacts" scale="yes" rect="60%,12,22x36" />
      </group>
      <rect rect="0,0,250x60"/>
      <group rect="0,0,105x37" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,dial()">
	<image src="big_dialer_btn" scale="yes" color="#007e00" onclick="src=big_dialer_btn_pressed" interactive="yes" />
        <text rect="0,0,100%x100%" bold="yes" color="#FFFFFF" size="8" align="hcenter,vcenter">Call</text>
      </group>
    </layout>

    <layout rect="50,105,360x350" orientation="vertical" spacing="10">
	    <layout rect="0,0,0x80" orientation="horizontal" spacing="20" align="hcenter">
