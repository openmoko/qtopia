		    </group>
		    <group rect="0,0,105x80" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),8">
			    <image scale="yes" src="dialer_btn" color="#006492" onclick="src=dialer_btn_pressed" interactive="yes" />
