<?xml version="1.0"?>
<page xmlns="http://www.trolltech.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.trolltech.com themedview.xsd" name="dialer" transparent="yes" base="themes/deskphone" keypad="no">

  <image name="background" rect="0,0,0x0" src="dialer_background" tile="yes"/>
  <image rect="5,5,310x33" src="number_bg"/>
  <dialer-lineedit-plugin name="dialernumber" rect="15,10,290x25" colorGroup="Background=#EEEEEE;Base=#DDDDDD;Text=#000000" bold="yes" size="15"/>
    <layout rect="200,40,115x150" orientation="vertical">
      <layout rect="0,0,0x37" orientation="horizontal" spacing="1" align="hcenter">
        <group rect="0,0,55x37" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,clear()">
          <image src="dialer_btn" color="#7e0000" onclick="src=dialer_btn_pressed" interactive="yes" />
          <text rect="0,10,0,0" bold="yes" color="#FFFFFF" size="9" align="hcenter">Clear</text>
        </group>
        <group rect="0,0,55x37" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,backspace()">
          <image src="dialer_btn" color="#7e0000" onclick="src=dialer_btn_pressed" interactive="yes" />
          <image rect="10,8,35x22" src="backspace.svg"/>
        </group>
      </layout>
      <group rect="0,0,105x37" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,saveToContact()">
        <image src="big_dialer_btn" color="#006492" onclick="src=big_dialer_btn_pressed" interactive="yes" />
        <text rect="0,10,0,0" bold="yes" color="#FFFFFF" size="10" align="hcenter">Add   </text>
        <image src="launch_contacts" scale="yes" rect="75,5,15x24" />
      </group>
      <rect rect="0,0,105x37"/>
      <group rect="0,0,105x37" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,dial()">
        <image src="big_dialer_btn" color="#007e00" onclick="src=big_dialer_btn_pressed" interactive="yes" />
        <text rect="0,10,0,0" bold="yes" color="#FFFFFF" size="10" align="hcenter">Call</text>
      </group>
    </layout>

    <layout rect="20,40,170x150" orientation="vertical">
      <layout rect="0,0,0x37" orientation="horizontal" spacing="1" align="hcenter">
        <group rect="0,0,55x37" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),1">
          <image src="dialer_btn" color="#006492" onclick="src=dialer_btn_pressed" interactive="yes" />
          <text rect="7,3,0,0" bold="yes" color="#FFFFFF" size="14">1</text>
        </group>
        <group rect="0,0,55x37" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),2">
          <image src="dialer_btn" color="#006492" onclick="src=dialer_btn_pressed" interactive="yes" />
          <text rect="7,3,0,0" bold="yes" color="#FFFFFF" size="14">2</text>
          <text rect="0,20,-7,0" bold="yes" color="#FFFFFF" size="8" align="right">ABC</text>
        </group>
        <group rect="0,0,55x37" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),3">
          <image src="dialer_btn" color="#006492" onclick="src=dialer_btn_pressed" interactive="yes" />
          <text rect="7,3,0,0" bold="yes" color="#FFFFFF" size="14">3</text>
          <text rect="0,20,-7,0" bold="yes" color="#FFFFFF" size="8" align="right">DEF</text>
        </group>
      </layout>
      <layout rect="0,0,0x37" orientation="horizontal" spacing="1" align="hcenter">
        <group rect="0,0,55x37" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),4">
          <image src="dialer_btn" color="#006492" onclick="src=dialer_btn_pressed" interactive="yes" />
          <text rect="7,3,0,0" bold="yes" color="#FFFFFF" size="14">4</text>
          <text rect="0,20,-7,0" bold="yes" color="#FFFFFF" size="8" align="right">GHI</text>
        </group>
        <group rect="0,0,55x37" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),5">
          <image src="dialer_btn" color="#006492" onclick="src=dialer_btn_pressed" interactive="yes" />
          <text rect="7,3,0,0" bold="yes" color="#FFFFFF" size="14">5</text>
          <text rect="0,20,-7,0" bold="yes" color="#FFFFFF" size="8" align="right">JKL</text>
        </group>
        <group rect="0,0,55x37" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),6">
          <image src="dialer_btn" color="#006492" onclick="src=dialer_btn_pressed" interactive="yes" />
          <text rect="7,3,0,0" bold="yes" color="#FFFFFF" size="14">6</text>
          <text rect="0,20,-7,0" bold="yes" color="#FFFFFF" size="8" align="right">MNO</text>
        </group>
      </layout>
      <layout rect="0,0,0x37" orientation="horizontal" spacing="1" align="hcenter">
        <group rect="0,0,55x37" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),7">
          <image src="dialer_btn" color="#006492" onclick="src=dialer_btn_pressed" interactive="yes" />
          <text rect="7,3,0,0" bold="yes" color="#FFFFFF" size="14">7</text>
          <text rect="0,20,-7,0" bold="yes" color="#FFFFFF" size="8" align="right">PQRS</text>
        </group>
        <group rect="0,0,55x37" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),8">
          <image src="dialer_btn" color="#006492" onclick="src=dialer_btn_pressed" interactive="yes" />
          <text rect="7,3,0,0" bold="yes" color="#FFFFFF" size="14">8</text>
          <text rect="0,20,-7,0" bold="yes" color="#FFFFFF" size="8" align="right">TUV</text>
        </group>
        <group rect="0,0,55x37" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),9">
          <image src="dialer_btn" color="#006492" onclick="src=dialer_btn_pressed" interactive="yes" />
          <text rect="7,3,0,0" bold="yes" color="#FFFFFF" size="14">9</text>
          <text rect="0,20,-7,0" bold="yes" color="#FFFFFF" size="8" align="right">WXYZ</text>
        </group>
      </layout>
      <layout rect="0,0,0x37" orientation="horizontal" spacing="1" align="hcenter">
        <group rect="0,0,55x37" name="star" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),*">
          <image src="dialer_btn" color="#006492" onclick="src=dialer_btn_pressed" interactive="yes" />
          <text rect="7,3,0,0" bold="yes" color="#FFFFFF" size="14">*</text>
        </group>
        <group rect="0,0,55x37" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),0">
          <image src="dialer_btn" color="#006492" onclick="src=dialer_btn_pressed" interactive="yes" />
          <text rect="7,3,0,0" bold="yes" color="#FFFFFF" size="14">0</text>
          <text rect="0,20,-7,0" bold="yes" color="#FFFFFF" size="8" align="right">OPER</text>
        </group>
        <group rect="0,0,55x37" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),#">
          <image src="dialer_btn" color="#006492" onclick="src=dialer_btn_pressed" interactive="yes" />
          <text rect="7,3,0,0" bold="yes" color="#FFFFFF" size="14">#</text>
        </group>
      </layout>
    </layout>
</page>
