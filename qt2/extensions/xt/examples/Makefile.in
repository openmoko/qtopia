SUBDIRS =	mainlyQt \
		mainlyXt \
		mainlyMotif

all:
	$(MAKE) $(SUBDIRS)


$(SUBDIRS): FORCE
	if [ -d $@ ]; then cd $@ ; $(MAKE); fi

clean:
	for i in $(SUBDIRS); do ( if [ -d $$i ]; then cd $$i ; $(MAKE) $@; fi; ) ; done

FORCE:
