export HOME=/home/user

[ ! -d $HOME ] && mkdir -p $HOME
# [ ! -e $HOME/Documents ]

export QPEDIR=/opt/Qtopia
export QTDIR=$QPEDIR
export PATH=$QPEDIR/bin:/usr/local/bin:/usr/local/sbin:$PATH
