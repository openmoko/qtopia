The following modules can be found in this Qtopia source package

devices: 
    example
    greenphone
etc: 
    themes: crisp, portal, qtopia
3rdparty: 
    applications: dbus, sqlite
    libraries: alsa, amr, dbus, gsm, helix, inputmatch, libtimidity, md5, openobex, openssl (crypto, ssl), qtdbus, sqlite, tar, zlib
    plugins: codecs (libamr, libtimidity), inputmethods (pkim)
applications: 
    addressbook
    calculator
    camera
    clock
    datebook
    helpbrowser
    mediaplayer
    mediarecorder
    photoedit
    qtmail
    sysinfo
    textedit
    todo
games: 
    fifteen
    minesweep
    qasteroids
    snake
libraries: 
    handwriting
    qtopia
    qtopiaaudio
    qtopiabase
    qtopiacomm
    qtopiacore: corelib, gui, network, sql, svg, xml
    qtopiail
    qtopiamail
    qtopiamedia
    qtopiapim
    qtopiaprinting
    qtopiasmil
plugins: 
    codecs: wavrecord
    content: exif, id3
    inputmethods: dockedkeyboard, keyboard
    mediadevices: builtin
    network: bluetooth, dialing, lan
    qtopiacore
    qtopiaprinting: bluetooth
